<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bokir.id">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Reward"
            android:parentActivityName=".MainActivity"
            android:theme="@style/SettingTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ScanActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/SettingTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".SettingActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/SettingTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".CariActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/SettingTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".RegisActivity" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".ContactActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/SettingTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".DetailPembayaran"
            android:parentActivityName=".CariActivity"
            android:theme="@style/Deskripsi">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CariActivity" />
        </activity>
        <activity android:name=".Pembayaran"
            android:theme="@style/SettingTheme"
            android:parentActivityName=".DetailPembayaran">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DetailPembayaran"
                />

        </activity>

        <activity android:name=".Riwayat"
            android:theme="@style/SettingTheme"
            android:parentActivityName=".MainActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />

        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>